{
    "name": "Chess24-IND",
    "description": "Modify Chess24 website to only show games of Indians",
    "version": "1.0",
    "manifest_version": 3,
    "permissions": [
        "scripting", 
        "tabs",
        "storage"
    ], 
    "host_permissions": [
        "http://*/",
        "https://*/"
    ],
    "action": {
        "default_popup": "index.html"
    }
}